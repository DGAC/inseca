{
  "build-dir": "{dir}/BUILD",
  "descr": "{descr}",
  "id": "{build}",
  "privdata-ekey-pub-file": "privdata-encrypt-key.pub",
  "privdata-ekey-priv-file": "privdata-encrypt-key.priv",
  "repo": "{repo}",
  "validity-months": 3,
  "version": "0.1",
  "i10n": {
    "timezone": "UTC",
    "locale": "en_US.UTF-8",
    "keyboard-layout": "en" ,
    "keyboard-model": "pc105",
    "keyboard-variant": null,
    "keyboard-option": null
  },
  "components": {
    "base-debian-bullseye": null,
    "gnome-desktop": {
      "display-type": "wayland"
    },
    "signature": {
      "build-skey-pub-file": "build-sign-key.pub",
      "build-skey-priv-file": "build-sign-key.priv"
    },
    "inseca-config": null,
    "sandboxed-programs": null,
    "inseca-live-wks": {
      "userdata-skey-pub-file": "userdata-sign-key.pub",
      "allow-network-connections": true,
      "allowed-virtualized": ""
    }
  }
}